{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "singleQuote": true,
  "printWidth": 100,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^vue$",
    "^vue-",
    "^@vue/",
    "^pinia$",
    "<THIRD_PARTY_MODULES>",
    "",
    "^.*\\.vue$",
    "^@/",
    "^[./]",
    "",
    "\\.css$"
  ]
}
